{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "games": {
            "type": "array",
            "items": [
                {
                    "$ref": "data/baseGame.json"
                }
            ]
        },
        "projects": {
            "type": "array",
            "items": [
                {
                    "$ref": "data/projects.json"
                }
            ]
        },
        "projectCount": {
            "type": "number"
        },
        "contributorCount": {
            "type": "number"
        }
    },
    "required": [
        "games",
        "projects",
        "projectCount",
        "contributorCount"
    ]
}