{
    "$schema": "https://json-schema.org/draft-07/schema",
    "type": "object",
    "properties": {
        "name": {
            "type": "string"
        },
        "slug": {
            "type": "string"
        },
        "url": {
            "type": "string"
        },
        "logoURL": {
            "$ref": "image.json"
        },
        "versions": {
            "$ref": "game_versions.json"
        },
        "projectCount": {
            "type": "number"
        },
        "projectTypes": {
            "type": "array",
            "items": {
                "$ref": "projectTypes.json"
            }
        },
        "sort": {
            "type": "array"
        },
        "defaultProjectType": {
            "type": "string"
        }
    },
    "required": [
        "name",
        "slug",
        "url",
        "logoURL"
    ],
    "additionalProperties": false
}