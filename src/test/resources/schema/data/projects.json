{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "id": {
            "type": "integer"
        },
        "name": {
            "type": "string"
        },
        "slug": {
            "type": "string"
        },
        "summary": {
            "type": "string"
        },
        "logo": {
            "type": "string"
        },
        "downloads": {
            "type": "integer"
        },
        "createdAt": {
            "type": "string"
        },
        "updatedAt": {
            "type": "string"
        },
        "tags": {
            "$ref": "tags.json"
        },
        "game": {
            "$ref": "baseGame.json"
        },
        "projectType": {
            "$ref": "baseProjectType.json"
        },
        "owner": {
            "$ref": "authors.json"
        },
        "authors": {
            "type": "array",
            "items": [
                {
                    "$ref": "authors.json"
                }
            ]
        }
    },
    "required": [
        "id",
        "name",
        "slug",
        "summary",
        "logo",
        "downloads",
        "createdAt",
        "updatedAt",
        "tags",
        "game",
        "projectType",
        "owner",
        "authors"
    ]
}