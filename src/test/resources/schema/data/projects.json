{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "name": {
            "type": "string"
        },
        "slug": {
            "type": "string"
        },
        "summary": {
            "type": "string"
        },
        "description": {
            "type": "string"
        },
        "logo": {
            "type": "string"
        },
        "downloads": {
            "type": "number"
        },
        "createdAt": {
            "type": "number"
        },
        "updatedAt": {
            "type": "number"
        },
        "contributors": {
            "$ref": "contributors.json"
        },
        "categories": {
            "$ref": "categories.json"
        },
        "links": {
            "type": "array",
            "items": [
                {
                    "type": "object",
                    "properties": {
                        "type": {
                            "type": "string"
                        },
                        "url": {
                            "type": "string"
                        }
                    },
                    "required": [
                        "type",
                        "url"
                    ],
                    "additionalProperties": false
                }
            ]
        }
    },
    "required": [
        "name",
        "slug",
        "summary",
        "downloads",
        "createdAt",
        "updatedAt",
        "contributors"
    ],
    "additionalProperties": false
}