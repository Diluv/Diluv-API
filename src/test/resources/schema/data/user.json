{
    "$schema": "https://json-schema.org/draft-07/schema",
    "type": "object",
    "properties": {
        "userId": {
            "type": "number"
        },
        "displayName": {
            "type": "string"
        },
        "username": {
            "type": "string"
        },
        "avatar": {
            "$ref": "image.json"
        },
        "createdAt": {
            "type": "string"
        },
        "email": {
            "type": "string"
        },
        "mfa": {
            "type": "boolean"
        }
    },
    "required": [
        "userId",
        "username",
        "displayName",
        "avatar",
        "createdAt"
    ],
    "additionalProperties": false
}