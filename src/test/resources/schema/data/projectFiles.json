{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "id": {
            "type": "number"
        },
        "name": {
            "type": "string"
        },
        "downloadURL": {
            "type": "string"
        },
        "changelog": {
            "type": "string"
        },
        "sha512": {
            "type": "string"
        },
        "size": {
            "type": "number"
        },
        "createdAt": {
            "type": "number"
        },
        "dependencies": {
            "type": "array"
        },
        "gameVersions": {
            "$ref": "game_versions.json"
        },
        "gameSlug": {
            "type": "string"
        },
        "projectTypeSlug": {
            "type": "string"
        },
        "projectSlug": {
            "type": "string"
        },
        "uploaderUserId": {
            "type": "number"
        },
        "uploaderUsername": {
            "type": "string"
        },
        "uploaderDisplayName": {
            "type": "string"
        },
        "updatedAt": {
            "type": "number"
        },
        "status": {
            "type": "string"
        },
        "lastStatusChanged": {
            "type": "number"
        },
        "classifier": {
            "type": "string"
        },
        "releaseType": {
            "type": "string"
        }
    },
    "required": [
        "id",
        "name",
        "sha512",
        "size",
        "createdAt",
        "gameSlug",
        "projectTypeSlug",
        "projectSlug",
        "uploaderUserId",
        "uploaderUsername"
    ],
    "additionalProperties": false
}