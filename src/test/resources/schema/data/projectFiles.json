{
    "$schema": "https://json-schema.org/draft-07/schema",
    "type": "object",
    "properties": {
        "id": {
            "type": "number"
        },
        "name": {
            "type": "string"
        },
        "downloadURL": {
            "type": "string"
        },
        "changelog": {
            "type": "string"
        },
        "sha512": {
            "type": "string"
        },
        "downloads": {
            "type": "number"
        },
        "size": {
            "type": "number"
        },
        "createdAt": {
            "type": "string"
        },
        "dependencies": {
            "$ref": "dependency.json"
        },
        "gameVersions": {
            "$ref": "game_versions.json"
        },
        "gameSlug": {
            "type": "string"
        },
        "projectTypeSlug": {
            "type": "string"
        },
        "projectSlug": {
            "type": "string"
        },
        "user": {
            "$ref": "user.json"
        },
        "updatedAt": {
            "type": "string"
        },
        "status": {
            "type": "string"
        },
        "lastStatusChanged": {
            "type": "string"
        },
        "classifier": {
            "type": "string"
        },
        "releaseType": {
            "type": "string"
        }
    },
    "required": [
        "id",
        "name",
        "sha512",
        "downloads",
        "size",
        "createdAt",
        "gameSlug",
        "projectTypeSlug",
        "projectSlug",
        "user"
    ],
    "additionalProperties": false
}