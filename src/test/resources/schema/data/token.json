{
    "$schema": "https://json-schema.org/draft-07/schema",
    "type": "object",
    "properties": {
        "userId": {
            "type": "number"
        },
        "username": {
            "type": "string"
        },
        "displayName": {
            "type": "string"
        },
        "apiToken": {
            "type": "boolean"
        },
        "permissions": {
            "type": "array",
            "items": [
                {
                    "type": "string"
                }
            ]
        }
    },
    "required": [
        "userId",
        "username",
        "displayName",
        "apiToken",
        "permissions"
    ],
    "additionalProperties": false
}