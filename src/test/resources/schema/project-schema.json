{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "data": {
            "type": "object",
            "properties": {
                "name": {
                    "type": "string"
                },
                "slug": {
                    "type": "string"
                },
                "summary": {
                    "type": "string"
                },
                "description": {
                    "type": "string"
                },
                "downloads": {
                    "type": "number"
                },
                "createdAt": {
                    "type": "number"
                },
                "updatedAt": {
                    "type": "number"
                },
                "contributors": {
                    "type": "array",
                    "items": [
                        {
                            "type": "object",
                            "properties": {
                                "username": {
                                    "type": "string"
                                },
                                "role": {
                                    "type": "string"
                                }
                            },
                            "required": [
                                "username",
                                "role"
                            ]
                        }
                    ]
                }
            },
            "required": [
                "name",
                "slug",
                "summary",
                "description",
                "downloads",
                "createdAt",
                "updatedAt",
                "contributors"
            ]
        }
    },
    "required": [
        "data"
    ]
}