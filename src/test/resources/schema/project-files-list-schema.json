{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "data": {
            "type": "array",
            "items": [
                {
                    "type": "object",
                    "properties": {
                        "id": {
                            "type": "number"
                        },
                        "name": {
                            "type": "string"
                        },
                        "sha512": {
                            "type": "string"
                        },
                        "crc32": {
                            "type": "string"
                        },
                        "size": {
                            "type": "number"
                        },
                        "changelog": {
                            "type": "string"
                        },
                        "createdAt": {
                            "type": "number"
                        },
                        "updatedAt": {
                            "type": "number"
                        }
                    },
                    "required": [
                        "id",
                        "name",
                        "sha512",
                        "crc32",
                        "size",
                        "changelog",
                        "createdAt",
                        "updatedAt"
                    ]
                }
            ]
        }
    },
